{
  "name": "apontador-js",
  "version": "1.0.0",
  "main": "api/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"cd app && npm start\"",
    "server": "nodemon api/index.js",
    "start": "node api/index.js",
    "build": "cd app && npm run build",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy",
    "seed": "node prisma/seed.js",
    "seed:csv": "node scripts/seed-csv.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "npx prisma generate",
    "build": "npx prisma generate",
    "vercel-build": "cd app && npm install && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.2.0",
    "prisma": "^5.6.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  }
}